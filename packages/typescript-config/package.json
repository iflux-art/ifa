{
  "name": "@repo/typescript-config",
  "version": "0.0.0",
  "private": true,
  "description": "Shared TypeScript configurations for the monorepo",
  "type": "module",
  "main": "index.js",
  "files": [
    "base.json",
    "nextjs.json",
    "react-library.json"
  ],
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./base": {
      "import": "./base.json"
    },
    "./base.json": {
      "import": "./base.json"
    },
    "./nextjs": {
      "import": "./nextjs.json"
    },
    "./nextjs.json": {
      "import": "./nextjs.json"
    },
    "./react-library": {
      "import": "./react-library.json"
    },
    "./react-library.json": {
      "import": "./react-library.json"
    }
  }
}